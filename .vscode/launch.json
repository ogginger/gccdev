{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch - Project (gdb)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/main",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
